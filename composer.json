{
    "name": "ejarnutowski/laravel-api-key",
    "description": "Authorize requests to your Laravel application with API keys",
    "keywords": ["api", "api key", "authentication", "authorization", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Elliott Jarnutowski",
            "email": "e@jarno.co"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "illuminate/console": "^5.5",
        "illuminate/database": "^5.5",
        "illuminate/routing": "^5.5",
        "illuminate/support": "^5.5",
        "illuminate/http": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Ejarnutowski\\LaravelApiKey\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ejarnutowski\\LaravelApiKey\\Providers\\ApiKeyServiceProvider"
            ]
        }
    }
}
